<!DOCTYPE html>
<html>
  <head>
    <title>unLeashd</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    "application", media: "all" %>
    <%= javascript_include_tag 'application' %>
    <%= favicon_link_tag asset_path('favicon.ico') %>
    <script>
      window.userIconURL = "<%= image_url('user-icon.png') %>";
      window.passwordIconURL = "<%= image_url('password-icon.png') %>";
      window.emailIconURL = "<%= image_url('email-icon.png') %>";
      window.defaultAva = "<%= image_url('default_avatar.png') %>";
      window.checkmark = "<%= image_url('checkmark.png') %>";
      window.whitex = "<%= image_url('white-x.png') %>";
      window.cameraURL = "<%= image_url('camera.png') %>";
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
